<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.productsystem.ProductController">
    <TableView fx:id="tableView" layoutX="20.0" layoutY="20.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="idColumn" minWidth="0.0" prefWidth="29.0" text="ID" />
            <TableColumn fx:id="nameColumn" minWidth="0.0" prefWidth="99.0" text="Name" />
            <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="78.0" text="Price" />
            <TableColumn fx:id="quantityColumn" minWidth="0.0" prefWidth="83.0" text="Quantity" />
            <TableColumn fx:id="imageColumn" minWidth="0.0" prefWidth="309.0" />
        </columns>
    </TableView>
</AnchorPane>
